<p id="notice"><%= notice %></p>

<h3>My Files</h3>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @assets.each do |asset| %>
      <tr>
        <td><%= asset.name %></td>
        <td><img src=<%= asset.model_image %> width="300px"</img></td>
        <td><%= link_to 'More Details', asset %> </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
